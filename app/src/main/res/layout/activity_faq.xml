<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.hetic.antoinegourtay.canieat.activity.FAQActivity">

    <TextView
        android:id="@+id/who_are_we"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:text="@string/quisommesnous"
        tools:layout_editor_absoluteY="0dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/happy_cow_explaination"
        tools:layout_editor_absoluteX="0dp"
        android:layout_marginTop="9dp"
        app:layout_constraintTop_toBottomOf="@+id/who_are_we" />

</android.support.constraint.ConstraintLayout>
